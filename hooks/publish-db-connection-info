echo "OPENSHIFT_MEMCACHED_PROXY_PORT1=$OPENSHIFT_MEMCACHED_PROXY_PORT1"
echo "OPENSHIFT_MEMCACHED_PROXY_PORT2=$OPENSHIFT_MEMCACHED_PROXY_PORT2"
echo "OPENSHIFT_MEMCACHED_PROXY_PORT3=$OPENSHIFT_MEMCACHED_PROXY_PORT3"